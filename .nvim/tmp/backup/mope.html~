   <script type ='text/javascript'>
      document.getElementById('bg').className = 'ontop';
      document.getElementById('anim').className = 'hide';
      var className = document.getElementById('load').className;
      document.getElementById('load').className = className.replace(/hide/, '');
      if (window.Sizzle) {
        var pages = Sizzle('.page');
        var menu = Sizzle('#menu');
      } else {
        var pages = document.getElementsByClassName('page');
        var menu =document.getElementById('menu');
      }
      menu.className += ' hide';
      for(var i = 0, len = pages.length; i < len; i++) {
        var page = pages[i];
        page.className += ' hide';
      }
    </script>
